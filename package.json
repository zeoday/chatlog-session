{
  "name": "chatlog-session",
  "version": "0.14.1",
  "description": "基于 Chatlog API 的微信聊天记录查看器",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "element-plus": "^2.11.8",
    "pinia": "^2.1.7",
    "pinyin-pro": "^3.27.0",
    "sharp": "^0.34.5",
    "terser": "^5.44.1",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.0",
    "prettier": "^3.1.1",
    "sass": "^1.94.0",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^6.0.0",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "chatlog",
    "wechat",
    "chat-history",
    "vue3",
    "typescript"
  ],
  "author": "Chatlog Session Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xlight/chatlog-session.git"
  },
  "bugs": {
    "url": "https://github.com/xlight/chatlog-session/issues"
  },
  "homepage": "https://github.com/xlight/chatlog-session#readme"
